<!DOCTYPE html>
<html lang="<%- lang %>">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
	<title><%- title %></title>
	<link rel="stylesheet" href="/c/mnuik.css">
	<link rel="stylesheet" href="/c/a.css">
	<script src="/j/p.js"></script>
</head>
<body>
	<% include header %>

	<nav>
		<ul itemscope itemtype="http://schema.org/SiteNavigationElement" id="nav-list">
			<% navs.forEach( function( item ) { %>
			<li>
				<a href="<%= item.url %>" itemprop="url">
					<object data="<%= item.icon %>.svg" type="image/svg+xml">
						<img src="<%= item.icon %>.100.png" srcset="<%= item.icon %>.125.png 1.25x, <%= item.icon %>.150.png 1.5x, <%= item.icon %>.175.png 1.75x, <%= item.icon %>.200.png 2x, <%= item.icon %>.250.png 2.5x, <%= item.icon %>.300.png 3x" alt="<%= item.title %>">
					</object>
					<span><%= item.title %></span>
				</a>
			</li>
			<% }) %>
		</ul>
	</nav>
		
	<div id="content">